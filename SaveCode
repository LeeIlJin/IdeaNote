https://www.onlinegdb.com/online_c++_compiler
#include <iostream>
#include <memory.h>

using namespace std;

void Alg(int);
bool Check(int);

int N, M;

int* nums;

int main()
{
    cin.tie(NULL);
    cout.tie(NULL);
    
    cin >> N >> M;
    
    nums = new int[M];
    memset(nums, 0, M * sizeof(int));
    
    Alg(0);
    
    
    delete[] nums;
    return 0;
}

bool Check(int c)
{
    return true;
}

void Alg(int c)
{
    if(c == M)
    {
        for(int i=0; i<M; i++)
            cout << nums[i] << " ";
        cout << "\n";
        return;
    }
    
    int s = (c == 0)? 0 : nums[c-1]-1;
    
    for(int i=s; i<N; i++)
    {
        nums[c] = i+1;
        if(Check(c))
            Alg(c+1);
    }
    
    return;
}

